{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/web/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "runtime": "vercel-node@latest",
            "memory": 1024,
            "maxDuration": 10
        }
    }
}